ources":{"styles.css":"@font-face {\n  font-family: \"Cursed Timer ULiL\";\n  src: url(\"cursed-timer-ulil-font/CursedTimerUlil-Aznm.ttf\");\n}\n\n@font-face {\n  font-family: \"Silkscreen\";\n  src: url(\"Silkscreen-fontfacekit/web fonts/silkscreen_normal_macroman/slkscr-webfont.woff\")\n    format(\"woff\");\n}\n\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n\nbody {\n  height: 100vh;\n  background-image: linear-gradient(#2c3e50, #bdc3c7);\n}\n\nh1 {\n  font-family: \"Silkscreen\", sans-serif;\n  font-size: 4em;\n  text-align: center;\n  color: white;\n  margin-top: 15px;\n}\n\n/*Game Window*/\n#gameWindow {\n  margin-top: 20px;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  background-color: #dcdcdc;\n  margin-left: auto;\n  margin-right: auto;\n  border: 4px outset #bebebe;\n}\n\n/*Header*/\n#header {\n  height: 70px;\n  width: 94%;\n  border: 4px inset #bebebe;\n  margin: 20px;\n  margin-top: 20px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.buttonContainer {\n  background-color: lightgray;\n  display: flex;\n  align-self: flex-start;\n}\n\n.dropdown {\n  position: relative;\n  display: inline-block;\n}\n\n.dropdownContent {\n  display: none;\n  position: absolute;\n}\n\n.dropItem {\n  border-top: 1px dashed black;\n  border-bottom: 1px dashed black;\n}\n\n.dropBtn {\n  color: purple;\n  text-decoration: underline;\n}\n\n.dropdown:hover .dropdownContent {\n  display: block;\n}\n\n.dropdown .dropItem:hover {\n  background-color: whitesmoke;\n}\n\nbutton {\n  margin-left: 8px;\n  width: 95px;\n  font-weight: normal;\n  font-size: 1em;\n  text-align: left;\n  border-radius: 0%;\n  border: none;\n  border-right: 1px solid black;\n  background-color: lightgray;\n  -webkit-user-select: none;\n  -webkit-touch-callout: none;\n}\n\n/*Grid Board*/\n#board {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: center;\n  margin-bottom: 20px;\n  border: 4px inset #bebebe;\n}\n\n.cell {\n  width: 35px;\n  height: 35px;\n  font-size: 1.4em;\n  font-weight: 900;\n  border: 4px outset #bebebe;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  -webkit-user-select: none;\n  -webkit-touch-callout: none;\n}\n\n.bombSprite,\n.flagSprite,\n.cellSprite {\n  width: 25px;\n  height: 25px;\n}\n\n.bombSprite {\n  display: none;\n}\n\n.headerItem {\n  width: 95px;\n  height: 50px;\n  background-color: black;\n  color: red;\n  font-family: \"Cursed Timer ULiL\", sans-serif;\n  font-size: 3em;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  margin-right: 8px;\n  margin-left: 8px;\n  -webkit-user-select: none;\n  -webkit-touch-callout: none;\n}\n\n#timer {\n  width: 95px;\n}\n\n#smileyface {\n  width: 50px;\n  height: 50px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  border: 3px outset #bebebe;\n  -webkit-user-select: none;\n  -webkit-touch-callout: none;\n}\n\n#face {\n  width: 50px;\n  height: 50px;\n}\n"},"lineCount":null}},"error":null,"hash":"b0185755f044865bd88bdf8c490cae7f","cacheData":{"env":{}}}